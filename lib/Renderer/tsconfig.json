{
  "root": true,
  "compilerOptions": {
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2017",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "webworker"
    ],
    "skipLibCheck": true,
    "removeComments": false
  }
}
